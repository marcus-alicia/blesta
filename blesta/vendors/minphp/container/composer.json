{
    "name": "minphp/container",
    "description": "A Dependency Injection Container based on Pimple",
    "homepage": "http://github.com/phillipsdata/minphp-container",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cody Phillips",
            "email": "therealclphillips@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "container-interop/container-interop": "^1.0",
        "pimple/pimple": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^2.2",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {"Minphp\\Container\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Minphp\\Container\\Tests\\": "tests"}
    }
}