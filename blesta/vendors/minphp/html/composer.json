{
    "name": "minphp/html",
    "description": "HTML Helper",
    "homepage": "http://github.com/phillipsdata/minphp-html",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cody Phillips",
            "email": "therealclphillips@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.2",
        "satooshi/php-coveralls": "~1.0"
    },
    "suggest": {
        "ext-mbstring": "May increase performance encoding characters"
    },
    "autoload": {
        "psr-4": {"Minphp\\Html\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Minphp\\Html\\Tests\\": "tests"}
    }
}