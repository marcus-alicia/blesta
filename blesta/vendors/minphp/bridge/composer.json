{
    "name": "minphp/bridge",
    "description": "Bridges minPHP 0.x to minPHP 1.x",
    "homepage": "http://github.com/phillipsdata/minphp-bridge",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cody Phillips",
            "email": "therealclphillips@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "minphp/container": "^2.1",
        "minphp/acl": "^2.0",
        "minphp/cache": "^1.0",
        "minphp/configure": "^2.0",
        "minphp/date": "^1.0",
        "minphp/form": "^1.0",
        "minphp/html": "^1.0",
        "minphp/input": "^1.0",
        "minphp/javascript": "^1.0",
        "minphp/language": "^1.1",
        "minphp/pagination": "^1.0",
        "minphp/record": "^3.0",
        "minphp/session": "^1.2.1",
        "minphp/xml": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.2",
        "php-coveralls/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": {
          "Minphp\\Bridge\\": "src"
        },
        "classmap": [
            "src/Components",
            "src/Helpers",
            "src/Lib"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    }
}
