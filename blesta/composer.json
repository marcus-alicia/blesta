    "require-dev": {
        "squizlabs/php_codesniffer": "~2.2",
        "phpunit/phpunit": "~8.5"
    },
    "suggest": {
        "ext-gd": "The gd extension is recommended for better image support during PDF generation.",
        "ext-gmp": "The gmp extension is highly recommended for better performance.",
        "ext-imap": "The imap extension is required to send and receive mail via SMTP and IMAP.",
        "ext-libxml": "The libxml extension is highly recommended as it may be required to interface with some systems.",
        "ext-mailparse": "The mailparse extension is required to parse incoming emails.",
        "ext-mbstring": "The mbstring extension is required by some optional features.",
        "ext-simplexml": "The simplexml extension is highly recommended as it may be required to interface with some systems.",
        "ext-zlib": "The zlib extension is highly recommend for better performance."
    },
    "config": {
        "vendor-dir": "vendors",
        "preferred-install": "dist",
        "platform": {
            "php": "7.2.34"
        },
        "allow-plugins": {
            "composer/installers": true,
            "blesta/composer-installer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.blesta.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/phillipsdata/microftps"
        }
    ],
    "extra": {
        "installer-paths": {
            "plugins/{$name}": ["type:blesta-plugin"],
            "components/gateways/merchant/{$name}": ["type:blesta-gateway-merchant"],
            "components/gateways/nonmerchant/{$name}": ["type:blesta-gateway-nonmerchant"],
            "components/messengers/{$name}": ["type:blesta-messenger"],
            "components/modules/{$name}": ["type:blesta-module"],
            "components/invoice_templates/{$name}": ["type:blesta-invoice-template"],
            "components/reports/{$name}": ["type:blesta-reports"]
        }
    },
    "autoload": {
        "psr-4": {
            "Blesta\\Core\\": "core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blesta\\Tests\\": "../tests/"
        }
    }
}
