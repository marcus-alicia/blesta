<table class="table">
    <tr>
        <td>
            <a target="_blank" href="<?php echo (isset($login_url) ? $this->Html->safe($login_url) : null);?>"><?php $this->_('CWatch.service_info.option_login');?></a>
        </td>
    </tr>
    <?php
    $i = 0;
    foreach ((isset($licenses) ? $licenses : []) as $license) {
    ?>
